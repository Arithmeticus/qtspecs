<!-- This markup is slightly idiosyncratic in order to support
     styling with CSS. -->
<p role="type-hierarchy-figure">
<ulist role="type-hierarchy type-colors">
   <item role="primitive special root">
      <ulist>
         <item role="primitive">
            <p role="item">
               <phrase>decimal</phrase>
            </p>
            <ulist>
               <item role="built-in-atomic">
                  <p role="first">
                     <phrase>integer</phrase>
                  </p>
               </item>
            </ulist>
         </item>
         <item role="primitive">
            <p role="item">
               <phrase>double</phrase>
            </p>
         </item>
         <item role="primitive">
            <p role="item">
               <phrase>float</phrase>
            </p>
         </item>
      </ulist>
   </item>
</ulist>

<ulist role="type-hierarchy type-colors">
  <item>
    <p role="root"><phrase>Supertype</phrase></p>
    <ulist>
      <item><p><phrase>subtype</phrase></p></item>
    </ulist>
  </item>
</ulist>
<ulist role="type-legend type-colors">
  <item role="built-in-atomic"><p><phrase>Built-in atomic types</phrase></p></item>
</ulist>
</p>
